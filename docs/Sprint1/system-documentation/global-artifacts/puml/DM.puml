@startuml
'https://plantuml.com/class-diagram
skinparam classAttributeIconSize 0
hide circle
hide methods



class Vaccine {
    - commercial_name
    - brand
    - type
    - technology
}

class Vaccine_Type {
    - disease
    - short_description
}

class Facility {
    - name
    - postal_adress
    - phone_number
    - email
    - website_adress
    - opening_hours
    - closing_hours
    - center_capacity
}

class Employee {
    - name
    - postal_adress
    - phone_number
    - email
}


class SNS_User {
    - name
    - date_birth
    - sex
    - postal_adress
    - phone_number
    - email
    - CC_number
    - SNS_user_number
}



class Vaccination_Appointment {
    - SNS_user_number
    - facility
    - status
    - date
    - time
}

class Current_Outbreak {
    - disease
    - start_date
    - end_date
    - is_current
}

class Community_Mass_Vaccination_Center {
}

class Healthcare_Center {
}

class Nurse {
    - name
    - postal_adress
    - phone_number
    - email
}

class Receptionist {
    - name
    - postal_adress
    - phone_number
    - email
}

class Event {
    - user_arrival_time
    - brand
    - lot_number
    - reaction_notes
}

class Administrator {
    - name
    - email
    - phone_number
}

class Vaccine_Certificate {
    - issue_day
}

Employee <|-- Nurse
Employee <|-- Receptionist

Facility <|-- Community_Mass_Vaccination_Center
Facility <|-- Healthcare_Center

Administrator "1" -right--> "1..*" Nurse: register

Administrator "1" -left--> "1..*" Receptionist: register

Administrator "1" -down--> "1..*" Vaccine: register

Administrator "1" -right--> "1..*" Community_Mass_Vaccination_Center: register

Administrator "1" -up--> "1..*" Healthcare_Center: register

Administrator "1" -down--> "1..*" Vaccine_Type: specify a

SNS_User "1" -down--> "0..1" Vaccination_Appointment: makes a

Nurse "1..*" -down--> "1" Community_Mass_Vaccination_Center:works in
Nurse "1..*" -left--> "1" Healthcare_Center:works in

Receptionist "1..*" -up--> "1" Community_Mass_Vaccination_Center:works in
Receptionist "1..*" -up--> "1" Healthcare_Center:works in

Current_Outbreak "1" -left--> "1" Vaccine_Type: correlated

Vaccine "0..*" -right--> "1" Vaccine_Type: has

Nurse "1" -down--> "0..*" Vaccine_Certificate: creates >

Vaccine_Certificate "0..1" -down--> "1" Event: issued for

Vaccination_Appointment "1" -down--> "1" Vaccine_Type: correlated

Vaccination_Appointment "1" -up--> "0..1" Event: generates

Event "0..*" -right--> "1" SNS_User: belongs

Event "1" -down--> "1" Vaccine_Type:has

SNS_User "1"  -left--> "0..1" Vaccine_Certificate: belongs




@enduml